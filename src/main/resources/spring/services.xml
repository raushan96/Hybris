<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="quoter" class="de.andre.springTests.TerminatorQuoter">
		<property name="message" value="be back"/>
	</bean>

	<bean class="de.andre.springTests.InjectRandomBeanPostProcessor" />

	<bean id="andre" class="de.andre.springTests.Andre">
		<property name="name" value="andre"/>
		<property name="age" value="15"/>
		<property name="address" ref="address"/>
		<property name="address.town" value="xrinsk"/>

		<property name="map">
			<map>
				<entry key="test" value="123" value-type="int"/>
				<entry key="test1" value="chlex"/>
			</map>
		</property>
	</bean>

	<alias name="andre" alias="haha"/>

	<bean id="address" class="de.andre.springTests.Address">
		<property name="city" value="Minsk"/>
	</bean>

</beans>